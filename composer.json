{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "webmakerr/webmakerr",
    "description": "A fast, modern Tailwind WordPress theme by Webmakerr.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeffrey van Rossum",
            "email": "jeffrey@vanrossum.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webmakerr\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0.2",
        "tailpress/framework": "^5.0.4",
        "automattic/jetpack-autoloader": "^2.10"
    },
    "require-dev": {
        "tailpress/cli": "^5.0"
    },
    "config": {
        "allow-plugins": {
            "automattic/jetpack-autoloader": true
        }
    }
}
